def numberOfLights ():
    rows, cols = (1000, 1000)
    arr = [[0 for i in range(cols)] for j in range(rows)]
    
    fileIn = open("input.txt", "r")
   

    s= 0 

    for each in fileIn:
        x = each.split(" ")
        
        if "on" in x:
            virgula = x[2].split(",")
            a1 = (int)(virgula[0])
            a2 = int(virgula[1])
            virgula = x[4].split(",")
            b1 = (int)(virgula[0])
            b2 = (int)(virgula[1])
            
            for i in range (a1,b1+1):
                for j in range(a2,b2+1):
                    arr[i][j]+=1
                        
            
         
        elif "off" in x:
            virgula = x[2].split(",")
            a1 = (int)(virgula[0])
            a2 = int(virgula[1])
            virgula = x[4].split(",")
            b1 = (int)(virgula[0])
            b2 = (int)(virgula[1])
            
            for i in range (a1,b1+1):
                for j in range(a2,b2+1):
                    if arr[i][j]>0: 
                        arr[i][j]-=1
        
        elif "toggle" in x:
            virgula = x[1].split(",")
            a1 = (int)(virgula[0])
            a2 = int(virgula[1])
            virgula = x[3].split(",")
            b1 = (int)(virgula[0])
            b2 = (int)(virgula[1])

            #print(x[1]+" "+x[3])
            
            for i in range (a1,b1+1):
                for j in range(a2,b2+1):
                    arr[i][j]+=2

        for i in range (0,1000):
            s = s+sum(arr[i])

    fileIn.close()

    return s




            

   
    
