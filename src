import string

from click import argument

def findFloor(string):
    floor = 0
    for c in string:
        if c == '(':
            floor+=1
        else: floor-=1
    
    return floor

def findCharacter(string):
    floor = 0
    pos = 1
    for c in string:
        if c == '(': floor+=1
        else: floor-=1

        if (floor == -1): return pos

        pos+=1
    
    return 0

###################################################################

def findDimension(l , w , h):

    minim = min(l*w , w*h , l*h)

    return minim+2*(l*w + w*h + l*h)

def findDimensionPartTwo(l , w , h):
    
    minim1 = min (l,w,h)
    minim2 = 0

    if minim1 == l:
        minim2 = min(w,h)
    elif minim1 == w: minim2 = min (l,h)
    elif minim1 == h: minim2 = min (w,l)

    return (minim1*2 + minim2*2 ) +(l*w*h)    
     


###################################################################

def numberOfHouses(string):
    
    dict = {}

    dict[(0,0)]=1

    l = 0 ; c = 0

    cont = 1

    for i in string:
        l , c = rep(i,l,c)

        if dict.get((l,c)) == None:
            #print("OK")
            dict[(l,c)]=1
            cont+=1

    return cont 

def rep(i , l , c):
    if i == '>': c+=1
    elif i == '<': c-=1
    elif i == '^': l-=1
    elif i == 'v': l+=1
    
    return (l,c)

def  numberOfHousesPartTwo(string):
    dict = {}

    dict[(0,0)]=1

    l1 = 0 ; c1 = 0
    l2 = 0 ; c2 = 0

    cont = 1 

    for i in range(0,len(string)):
        if i %2 ==0 : 
            l1 , c1 = rep(string[i],l1,c1)
            if dict.get((l1,c1)) == None:
                dict[(l1,c1)]=1
                cont+=1

        else: 
            l2 , c2 = rep(string[i],l2,c2)
            if dict.get((l2,c2)) == None:
                dict[(l2,c2)]=1
                cont+=1
    
    return cont
        
