#if __name__ == "__main__": main()

from math import e,exp

def findArray(X):
    k = len(X)

    allSum =0.0 
    
    for j in range(1,k+1):
        allSum+= exp(e,X[j])

    res=[] 

    for i in range(0,k):
        res[i] = ((exp(e,X[i])) / allSum)
    
    return res
