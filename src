def nice(string ):

    v = 0
    ok1 = False

    for i in range(0,len(string)):
        if string[i].lower() in ('a', 'e', 'i', 'o', 'u'):
            v+=1
        
        if i>0 and (string[i-1]==string[i]):
            ok1=True
    
    ok2 = ("ab" in string) or ("cd" in string) or ("pq" in string) or ("xy" in string) 

    return (v>=3) and  ok1 and (not ok2)

def nicePartTwo(string):
    ok1 = False
    ok2 = False

    dict = {}

    for i in range(0,len(string)):
        #case1:

        if(i+1<len(string) ):
            key = string[i]+string[i+1]
            if(dict.get(key)==None):
                dict[key]=(i+1)
            else:
                last = dict.get(key)
                if i > last: ok1 = True
            
        #case2:
        if(i+1<len(string) and i+2<len(string)):
            if(string[i]!=string[i+1] and string[i]==string[i+2]):
                ok2=True
    
    #print(str(ok1)+"  "+str(ok2))
    #print(dict)

    return ok1 and ok2

