def lookAndSay(string):
    i=0
    rez=""

    while i<len(string):
        lc = 1
        j=i+1
        while(j<len(string) and string[j]==string[i]):
            lc+=1
            j+=1
        
        rez+=(str(lc)+string[i])
        i=j
    return rez


