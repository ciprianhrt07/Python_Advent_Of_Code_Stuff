             if "\n" in list[1]:
                list[1] = list[1].rstrip(list[1][-1])
             key = list[1]
             key = key[1::]
             dict[key]=value 


    nbNec = len(dict.keys())         

    for key in dict.keys(): 
        value = dict.get(key)
        if (checkNumber(value)):
            print(key+" : "+value)   
            nbNec-=1


