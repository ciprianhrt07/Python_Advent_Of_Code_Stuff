def computeString(string):
    
    file = open("input.txt", "rb")

    byte = file.read(1)

    input =[] 

    listOne = []
    listTwo =[]
    
    while byte:
        #print(byte,end=" ")    
        input.append(byte)

        if byte == b'\n':
            input.pop()
            #print(input,end=" ")
            #print(len(input)) 
            listOne.append(len(input)) 
            input=[]
        
        byte = file.read(1)
    
    file.close()

    file = open ("input.txt","r")

    """1)
    for each in file:
        aux = each
        aux = aux.rstrip(aux[-1])
        aux = aux.rstrip(aux[-1])
        aux = aux[1:]

        cnt = 0
        i = 0

        while i<len(aux):
            if (i+1<len(aux)) and (aux[i]=='\\' and aux[i+1]=='\"'):
                cnt+=1
                i+=1
            elif (i+1<len(aux)) and (aux[i]=='\\' and aux[i+1]=='\\'):
                cnt+=1
                i+=1
            elif (i+1<len(aux)) and (aux[i]=='\\' and aux[i+1]=='x'):
                cnt+=1
                i+=3
            else:
                cnt+=1

            i+=1
        
        listTwo.append(cnt)  
    """

    for each in file:
        aux = each
        aux = aux.rstrip(aux[-1])
        
        i = 0
        cnt =0

        while i< len(aux):
            if aux[i] == "\"":
                cnt+=3
            elif (i+1 <len(aux)) and (aux[i]=='\\' and aux[i+1]=='\\'):   
                cnt+=4
                i+=1
            elif (i+1 <len(aux)) and (aux[i]=='\\' and aux[i+1]=='\"'):
                cnt+=4
                i+=1
            elif (i+1 <len(aux)) and (aux[i]=='\\' and aux[i+1]=='x'): 
                cnt+=5
                i+=3
            else:
                cnt+=1
            i+=1
        
        listTwo.append(cnt)

    listTwo.pop()

    return sum(listTwo)-sum(listOne)
    
             

